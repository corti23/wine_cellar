<% provide(:title, 'Sign up') %>
<% provide(:button_text, 'Create my account') %>
<h1>Sign up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= render 'form' %>
    <br>
    <a href="<%= google_path %>" class="google btn btn-danger btn-block"> <span></span><%= "Log in with your Google account" %></a>
    <br>
    <%= link_to image_tag("google.png", alt: "Log in with your Google account"), google_path %>
    
  </div>
</div>